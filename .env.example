# Server Configuration
PORT=5000
NODE_ENV=development

# API Configuration
API_RATE_LIMIT_WINDOW_MS=900000
API_RATE_LIMIT_MAX_REQUESTS=100

# Cache Configuration
CACHE_TTL_SECONDS=300
CACHE_CHECK_PERIOD_SECONDS=120

# External API
NSE_DATA_SOURCE_URL=https://afx.kwayisi.org/nse/
REQUEST_TIMEOUT_MS=30000
MAX_RETRIES=3
RETRY_DELAY_MS=2000

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=logs/app.log

# API Documentation
API_TITLE=NSE Scraper API
API_VERSION=2.0.0
API_DESCRIPTION=A fast and reliable API for scraping Nairobi Securities Exchange stock prices
